{
    "presets": [
        "react",
        "es2015",
        
        ["env", {
            "modules": false,
            "targets": {
                "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            }
        }],
        "stage-2"
    ],
    "plugins": [
        ["transform-runtime", { "polyfill": true }],
        "transform-decorators-legacy",
        "glamor/babel"
    ],
    "env": {
        "test": {
            "presets": ["env", "stage-2"]
        }
    }
}